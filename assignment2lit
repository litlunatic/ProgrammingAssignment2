makeCacheMatrix <- function( m = matrix() ) {

	## Initialize nverse property
    i <- NULL
set <- function( matrix ) {
            m <<- matrix
            i <<- NULL
    }
    ## get the matrix
    get <- function() {
    	## Return matrix
    	m
    }
## inverse of the matrix
    setInverse <- function(inverse) {
        i <<- inverse
    }

    ##  inverse of the matrix
    getInverse <- function() {
        ## Back the inverse property
        i
    }
## Back to list of the methods
    list(set = set, get = get,
         setInverse = setInverse,
         getInverse = getInverse)
}
## Compute the inverse of the unique matrix back by "makeCacheMatrix"
## Back to matrix  “m”
    m <- x$getInverse()
    if( !is.null(m) ) {
            message("getting cached data")
            return(m)
    }
    ## Compute the inverse via matrix multiplication
    m <- solve(data) %*% data
##  inverse to the object
    x$setInverse(m)
## back to the matrix
    m
}
